# 4K Random Read Test - Measures small random I/O performance
[global]
ioengine=libaio
direct=1
runtime=30
time_based=1
group_reporting=1
lat_percentiles=1
clat_percentiles=1
percentile_list=50:90:95:99:99.9:99.99:99.999

[4k-random-read]
filename=/dev/ublkb0
rw=randread
bs=4k
numjobs=1
iodepth=32
size=100%
