# 128K Sequential Read/Write Test - Measures throughput
[global]
ioengine=libaio
direct=1
runtime=30
time_based=1
group_reporting=1
lat_percentiles=1
clat_percentiles=1
percentile_list=50:90:95:99:99.9:99.99:99.999

[128k-seq-read]
filename=/dev/ublkb0
rw=read
bs=128k
numjobs=1
iodepth=32
size=100%

[128k-seq-write]
filename=/dev/ublkb0
rw=write
bs=128k
numjobs=1
iodepth=32
size=100%
